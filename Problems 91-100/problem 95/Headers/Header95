#include <iomanip>
#include <algorithm>
#include <fstream>
#include <string>
#include <vector>
#include <sstream>
#include <deque>
#include <ctime>
#include <cstring>
#include <cstdlib>
#include <map>
using namespace std;

int sumdiv(int r) {
	int total = 0;
	for (int i = 1; i*i <= r; i++) {
		if (r % i == 0) {
			if (i == r / i) {
				total = i + total;
			}
			else {
				total = i + r / i + total;
			}
		}
	}
	return total;  // Determines sum of a number's divisors.
}
