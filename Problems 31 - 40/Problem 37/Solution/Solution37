using namespace std;


int main() {
	unsigned t0 = clock();
	cout << 0 << endl;
int trunctotal = 0;
for (long long q = 11; q < 1000000; q++) {

	if (isTruncleft(q) && isTruncright(q)) {
		cout << "+ " << q << endl;
		trunctotal += q;
	}
}


std::cout << "= "<<trunctotal << endl;
unsigned elapsed = clock() - t0;
std::cout << "Elapsed time: " << elapsed << endl;
std::system("pause");
return 0;

}
