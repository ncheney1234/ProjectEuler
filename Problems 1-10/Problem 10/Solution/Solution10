using namespace std;

int main() {
	unsigned t0 = clock();
	long long sum = 2; //2 is prime
	for (long long i = 3; i < 2000000; i+=2) {
		if (isPrime(i)) { sum += i; }

	}

	std::cout << sum << endl;
	unsigned elapsed = clock() - t0;
	std::cout << "Elapsed time: " << elapsed << endl;
	std::system("pause");
	return 0;

}
