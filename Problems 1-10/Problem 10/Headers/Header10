#include <iostream>
#include <cstring>
#include <cstdlib>
#include <map>
using namespace std;


bool isPrime(unsigned long long n) {
	if (n <= 3) {
		return n > 1;
	}

	if (n % 2 == 0 || n % 3 == 0) {
		return false;
	}

	for (unsigned long long i = 5; i * i <= n; i += 6) {
		if (n % i == 0 || n % (i + 2) == 0) {
			return false;
		}
	}

	return true;
}
