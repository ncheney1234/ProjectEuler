#include "Header30.h"
using namespace std;

int main() {
	unsigned t0 = clock();
	long long total = 0;
	for (long long i = 2; i < 354294; i++) {
		if (i == sumpowerdigits(i, 5)) {
			total += i;
		}

	}

	std::cout << total<< endl;
	unsigned elapsed = clock() - t0;
	std::cout << "Elapsed time: " << elapsed << endl;
	std::system("pause");
	return 0;

}
