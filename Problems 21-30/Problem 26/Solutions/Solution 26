using namespace std;

int main() {
	unsigned t0 = clock();

int mostcycles = 1;
int d = 1;
for (int n = 1; n < 1000; n++) {

	if (recipricolcycles(n)> mostcycles) {
		mostcycles = recipricolcycles(n);
		d = n;
		

	}
}

std::cout << "1/" << d << " has " << mostcycles << " cycles" << endl;
unsigned elapsed = clock() - t0;
std::cout << "Elapsed time: " << elapsed << endl;
std::system("pause");
return 0;

}
